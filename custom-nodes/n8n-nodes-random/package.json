{
  "name": "n8n-nodes-random",
  "version": "1.0.0",
  "description": "Custom n8n node for generating true random numbers using Random.org API",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "random",
    "numbers"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Arthur",
    "email": "your.email@example.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/n8n-nodes-random.git"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "npm@10.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc && npm run copy-icons",
    "copy-icons": "cp -r nodes/**/*.svg dist/nodes/",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -s"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/Random/Random.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "n8n-workflow": "^1.0.0"
  }
}